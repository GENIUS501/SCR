                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Punto_Venta_Cliente.Cedula_juridica_fisica, Punto_Venta_Cliente.Nombre, Vencimiento_Productos.Id, Vencimiento_Productos.Fecha_reporte_vencimiento, Vencimiento_Productos.Fecha_vencimiento_producto, 
                  Vencimiento_Productos.tiempo_reaccion, Vencimiento_Productos.Cedula_Supervisor, Vencimiento_Productos.Zona, Vencimiento_Productos.Cliente, Vencimiento_Productos.Descripcion_SKU, Vencimiento_Productos.UnidadesxSKU, 
